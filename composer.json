{
    "name": "yangweijie/think-orm",
    "description": "the PHP Database&ORM Framework",
    "keywords": [
        "orm",
        "database"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*",
        "ext-json": "*",
        "ext-pdo": "*",
        "psr/log": ">=1.0",
        "psr/simple-cache": "1.0.1",
        "topthink/think-helper": "^3.1",
        "topthink/think-validate": "2.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src"
        },
        "files": [
            "src/helper.php",
            "stubs/load_stubs.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "suggest": {
        "ext-mongodb": "provide mongodb support"
    },
    "config": {
        "sort-packages": true
    }
}
